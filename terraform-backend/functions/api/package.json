{
  "name": "oriter-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "local": "dotenv -e .env.local -- nodemon ./src/server.ts",
    "build": "tsc --build && cp package.json dist/ && mkdir dist/node_modules && npm install --omit=dev --prefix dist && cd dist && zip -r ../oriter_api.zip .",
    "typecheck": "tsc --noEmit",
    "db:format": "npx prisma format --schema=../../../../prisma/schema.prisma",
    "db:reset": "dotenv -e .env.local -- npx prisma migrate reset --schema=../../prisma/schema.prisma",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev --schema=../../prisma/schema.prisma",
    "db:generate": "dotenv -e .env.local -- npx prisma generate --schema=../../prisma/schema.prisma",
    "db:seed": "dotenv -e .env.local -- npx prisma db seed --schema=../../prisma/schema.prisma",
    "db:studio": "dotenv -e .env.local -- npx prisma studio --schema=../../prisma/schema.prisma",
    "db:mock": "dotenv -e .env.local -- ts-node ./src/mocks/index.ts"
  },
  "prisma": {
    "seed": "ts-node --transpile-only ./src/config/db/seed.ts"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.134",
    "@types/aws-serverless-express": "^3.3.9",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.11.2",
    "@types/uuid": "^9.0.8",
    "aws-serverless-express": "^3.4.0",
    "bestzip": "^2.2.1",
    "dotenv": "^16.4.5",
    "morgan": "^1.10.0",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@prisma/client": "^5.11.0",
    "aws-lambda": "^1.0.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "formidable": "^3.5.1",
    "generate-password": "^1.7.1",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "serverless-express": "^2.0.12"
  },
  "keywords": [],
