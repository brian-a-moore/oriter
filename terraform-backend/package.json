  "main": "index.js",
  "scripts": {
    "local": "dotenv -e .env.local -- nodemon ./src/server.ts",
    "build": "tsc --build && cp package.json dist/ && mkdir dist/node_modules && npm install --omit=dev --prefix dist && cd dist &&  zip -r ../oriter_code.zip .",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint .eslintrc --quiet --fix",
    "prettier": "npx prettier --config .prettierrc --write .",
    "test": "sam build && sam local invoke OriterFormSubmission",
    "db:migrate": "npx drizzle-kit generate:pg",
    "db:push": "npx drizzle-kit push:pg",
    "db:push:local": "dotenv -e .env.local -- npx drizzle-kit push:pg"
  },
